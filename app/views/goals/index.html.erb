<a href="<%= new_goal_path %>">New Goal</a>
<% @goals.each do |goal| %>
  <a href="<%= edit_goal_path(goal.id) %>"><span class="glyphicon glyphicon-pencil"></span></a>
  <a href="<%= goal_path(goal.id) %>" data-method="delete"><span class="glyphicon glyphicon-trash"></span></a>
      <a role="button" data-toggle="collapse" href=".collapseExample" aria-expanded="false" aria-controls="collapseExample">
        <%= goal.goal_title %>
        <%= goal.priority %>
        <%= image_tag goal.goal_img.url(:medium) %>
        <%= goal.completed %>
      </a>
        <% goal.entries.each do |entry| %>
      <div class="collapse collapseExample">
        <div class="well">
          <%= entry.entry_title %>
          <%= entry.entry_date %>
        </div>
      </div>
  <% end %>
  <a href="<%= new_goal_entry_path(goal.id) %>">Add Entry</a>
<% end %>
