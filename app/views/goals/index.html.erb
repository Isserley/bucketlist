<a href="<%= new_goal_path %>">New Goal</a>
<% @goals.each do |goal| %>
  <a href="<%= edit_goal_path(goal.id) %>"><span class="glyphicon glyphicon-pencil"></span></a>
  <a href="<%= goal_path(goal.id) %>" data-method="delete"><span class="glyphicon glyphicon-trash"></span></a>
  <%= goal.goal_title %>
  <%= goal.priority %>
  <%= image_tag goal.goal_img.url(:medium) %>
  <%= goal.completed %>
  <a href="<%= new_goal_entry_path(goal.id) %>">Add Entry</a>
<% end %>
